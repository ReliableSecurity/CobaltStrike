sub akuma_deploy_all_beacons {
    local('@beacons', '$beacon');
    @beacons = beacons();
    
    println("ğŸš€ AKUMA: Deploying to all " . size(@beacons) . " beacons...");
    
    foreach $beacon (@beacons) {
        if (beacon_info($beacon, "alive") eq "true") {
            blog($beacon, "ğŸš€ AKUMA Auto-deployment starting...");
            akuma_rapid_full($beacon);
        }
    }
    
    println("âœ… AKUMA: Deployment complete on all active beacons!");
}

sub akuma_show_deployment_status {
    local('@beacons', '$beacon', '$info');
    @beacons = beacons();
    
    println("ğŸ“Š AKUMA Deployment Status:");
    println("=" x 50);
    
    foreach $beacon (@beacons) {
        $info = beacon_info($beacon, "computer") . "@" . beacon_info($beacon, "user");
        if (beacon_info($beacon, "alive") eq "true") {
            println("âœ… " . $info . " - ONLINE");
        } else {
            println("âŒ " . $info . " - OFFLINE");
        }
    }
}

